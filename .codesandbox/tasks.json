{
  // These tasks will run in order when initializing your CodeSandbox project.
  "setupTasks": [
    {
      "name": "Install Dependencies",
      "command": "npm install"
    }
  ],

  // These tasks can be run from CodeSandbox. Running one will open a log in the app.
  "tasks": {
    "clean-nm": {
      "name": "clean-nm",
      "command": "npm run clean-nm"
    },
    "clean-build": {
      "name": "clean-build",
      "command": "npm run clean-build"
    },
    "clean-dist": {
      "name": "clean-dist",
      "command": "npm run clean-dist"
    },
    "bundle": {
      "name": "bundle",
      "command": "npm run bundle"
    },
    "build": {
      "name": "build",
      "command": "npm run build"
    },
    "watch": {
      "name": "watch",
      "command": "npm run watch"
    },
    "dev": {
      "name": "dev",
      "command": "npm run dev",
      "runAtStart": true
    },
    "production": {
      "name": "production",
      "command": "npm run production"
    },
    "lint:sass": {
      "name": "lint:sass",
      "command": "npm run lint:sass"
    },
    "lint:js": {
      "name": "lint:js",
      "command": "npm run lint:js"
    },
    "stats": {
      "name": "stats",
      "command": "npm run stats"
    },
    "CSB_RUN_OUTSIDE_CONTAINER=1 devcontainer templates apply --template-id \"ghcr.io/devcontainers/templates/javascript-node\" --template-args '{}' --features '[]'": {
      "name": "Environment setup",
      "command": "CSB_RUN_OUTSIDE_CONTAINER=1 devcontainer templates apply --template-id \"ghcr.io/devcontainers/templates/javascript-node\" --template-args '{}' --features '[]'"
    }
  }
}
